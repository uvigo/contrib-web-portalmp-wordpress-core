{
  "name": "contrib-web-portalmp-wordpress-core",
  "version": "1.4.1",
  "description": "Plugin UVigo with basic and common elements",
  "engines": {
    "node": ">= 6.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uvigo/contrib-web-portalmp-wordpress-core.git"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "uvigo"
  ],
  "author": "IdeiT <info@ideit.es>",
  "contributors": [
    "uvigo-atic <atic@uciugo.es>",
    "Fernando Gonz√°lez <fernando.gonzalez@ideit.es>"
  ],
  "license": "GPL-2.0-or-later",
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "scripts": {
    "build": "webpack --progress --env.ZONE=public --config build/webpack.config.js",
    "build:production": "webpack --env.production --env.ZONE=public --progress --config build/webpack.config.js",
    "build:admin": "webpack --progress --env.ZONE=admin --config build/webpack.config.js",
    "build:admin:production": "webpack --progress --env.production --env.ZONE=admin --config build/webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config build/webpack.config.js",
    "start": "webpack --env.ZONE=public --hide-modules --watch --config build/webpack.config.js",
    "start:admin": "webpack --env.ZONE=admin --hide-modules --watch --config build/webpack.config.js",
    "rmdist": "rimraf dist",
    "lint": "npm run -s lint:scripts && npm run -s lint:scripts:admin && npm run -s lint:styles && npm run -s lint:styles:admin",
    "lint:scripts": "eslint public/assets/scripts build",
    "lint:scripts:admin": "eslint admnin/assets/scripts build",
    "lint:styles": "stylelint \"public/assets/styles/**/*.{css,sass,scss,sss,less}\"",
    "lint:styles:admin": "stylelint \"admin/assets/styles/**/*.{css,sass,scss,sss,less}\"",
    "deploy": "npm run -s build:production && npm run -s build:admin:production && node build/deploy.js",
    "test": "npm run -s lint"
  },
  "devDependencies": {
    "autoprefixer": "~8.2.0",
    "bestzip": "^2.1.1",
    "browser-sync": "^2.26.7",
    "browsersync-webpack-plugin": "^0.6.0",
    "bs-html-injector": "~3.0",
    "buble-loader": "^0.4.1",
    "cache-loader": "~1.2.0",
    "clean-webpack-plugin": "^0.1.18",
    "copy-globs-webpack-plugin": "^0.3.0",
    "css-loader": "^3.0.0",
    "cssnano": "~4.0.5",
    "eslint": "~4.19.1",
    "eslint-loader": "~1.9",
    "eslint-plugin-import": "~2.14.0",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "^1.1.6",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "import-glob": "~1.5",
    "node-sass": "^7.0.0",
    "postcss-loader": "~2.1.0",
    "postcss-safe-parser": "~3.0",
    "resolve-url-loader": "~2.3.0",
    "rimraf": "~2.6",
    "sass-loader": "~6.0",
    "style-loader": "^0.22.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.6.2",
    "webpack": "~3.11.0",
    "webpack-assets-manifest": "^1.0.0",
    "webpack-dev-middleware": "~2.0.4",
    "webpack-hot-middleware": "~2.22.3",
    "webpack-merge": "~4.1.4",
    "yargs": "~11.0.0"
  },
  "dependencies": {
    "contrib-web-portalmp-iconfont": "github:uvigo/contrib-web-portalmp-iconfont"
  }
}
